								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"method": "GET"
							},
							"linkedServiceName": {
								"referenceName": "itrefundweb",
								"type": "LinkedServiceReference"
