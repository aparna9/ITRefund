{
	"name": "ITRefundPipeline",
	"properties": {
		"activities": [
			{
				"name": "itrefundweb",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://api.fiscaldata.treasury.gov/services/api/fiscal_service/v1/accounting/dts/income_tax_refunds_issued?page[size]=8000&page[number]=1",
					"method": "GET"
				}
			},
			{
				"name": "itrefundloop",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "itrefundweb",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@range(1, int(activity('itrefundweb').output['meta']['total-pages']))\n",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "itrefundcopy",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "RestSource",
									"httpRequestTimeout": "00:01:40",
									"requestInterval": "00.00:00:00.010",
									"requestMethod": "GET",
									"paginationRules": {
										"supportRFC5988": "true"
									}
								},
								"sink": {
									"type": "AzurePostgreSQLSink",
									"writeBatchSize": 1000000,
									"writeBatchTimeout": "00:30:00",
									"writeMethod": "CopyCommand"
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"mappings": [
										{
											"source": {
												"path": "['record_date']"
											},
											"sink": {
												"name": "record_date",
												"type": "DateTime"
											}
										},
										{
											"source": {
												"path": "['tax_refund_type']"
											},
											"sink": {
												"name": "tax_refund_type",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['tax_refund_type_desc']"
											},
											"sink": {
												"name": "tax_refund_type_desc",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['tax_refund_today_amt']"
											},
											"sink": {
												"name": "tax_refund_today_amt",
												"type": "Decimal"
											}
										},
										{
											"source": {
												"path": "['tax_refund_mtd_amt']"
											},
											"sink": {
												"name": "tax_refund_mtd_amt",
												"type": "Decimal"
											}
										},
										{
											"source": {
												"path": "['tax_refund_fytd_amt']"
											},
											"sink": {
												"name": "tax_refund_fytd_amt",
												"type": "Decimal"
											}
										},
										{
											"source": {
												"path": "['table_nbr']"
											},
											"sink": {
												"name": "table_nbr",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['table_nm']"
											},
											"sink": {
												"name": "table_nm",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['sub_table_name']"
											},
											"sink": {
												"name": "sub_table_name",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['src_line_nbr']"
											},
											"sink": {
												"name": "src_line_nbr",
												"type": "Int32"
											}
										},
										{
											"source": {
												"path": "['record_fiscal_year']"
											},
											"sink": {
												"name": "record_fiscal_year",
												"type": "Int32"
											}
										},
										{
											"source": {
												"path": "['record_fiscal_quarter']"
											},
											"sink": {
												"name": "record_fiscal_quarter",
												"type": "Int32"
											}
										},
										{
											"source": {
												"path": "['record_calendar_year']"
											},
											"sink": {
												"name": "record_calendar_year",
												"type": "Int32"
											}
										},
										{
											"source": {
												"path": "['record_calendar_quarter']"
											},
											"sink": {
												"name": "record_calendar_quarter",
												"type": "Int32"
											}
										},
										{
											"source": {
												"path": "['record_calendar_month']"
											},
											"sink": {
												"name": "record_calendar_month",
												"type": "Int32"
											}
										},
										{
											"source": {
												"path": "['record_calendar_day']"
											},
											"sink": {
												"name": "record_calendar_day",
												"type": "Int32"
											}
										}
									],
									"collectionReference": "$['data']",
									"mapComplexValuesToString": true
								}
							},
							"inputs": [
								{
									"referenceName": "itrefundrest",
									"type": "DatasetReference",
									"parameters": {
										"param": {
											"value": "?page[size]=8000&page[number]=@{item()}",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "itrefundsourcetables",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			},
			{
				"name": "itrefundcountcheck",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzurePostgreSqlSource",
						"query": "select count(*) from itrefund_source;",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "itrefundsourcetables",
						"type": "DatasetReference"
					}
				}
			},
			{
				"name": "itrefundcondcheck",
				"type": "IfCondition",
				"dependsOn": [],
				"userProperties": []
			}
		],
		"annotations": []
	}
}